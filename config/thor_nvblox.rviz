Panels:
  - Class: rviz_common/Displays
    Help Height: 78
    Name: Displays
    Property Tree Widget:
      Expanded:
        - /Global Options1
        - /Status1
        - /TSDF Map1
        - /Mesh1
        - /Camera1
      Splitter Ratio: 0.5
    Tree Height: 523
  - Class: rviz_common/Time
    Name: Time
    SyncMode: 0
    SyncSource: ""
Preferences:
  PromptSaveOnExit: true
Toolbars:
  toolButtonStyle: 2

Visualization Manager:
  Class: ""
  Enabled: true
  Name: root

  Global Options:
    Background Color: 48; 48; 48
    Default Light: true
    Fixed Frame: map
    Frame Rate: 30

  Displays:
    - Class: rviz_default_plugins/Grid
      Name: Grid
      Enabled: true
      Alpha: 0.5
      Cell Size: 1
      Color: 160; 160; 164
      Normal Cell Count: 0
      Offset:
        X: 0
        Y: 0
        Z: 0
      Plane: XY
      Plane Cell Count: 50
      Reference Frame: <Fixed Frame>
      Line Style:
        Line Width: 0.029999999329447746
        Value: Lines
      Value: true

    - Class: rviz_default_plugins/MarkerArray
      Name: TSDF Map
      Enabled: true
      Namespaces:
        {}
      Queue Size: 100
      Topic:
        Value: /nvblox_node/tsdf_map
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
      Value: true

    - Class: rviz_default_plugins/MarkerArray
      Name: Mesh
      Enabled: true
      Namespaces:
        {}
      Queue Size: 100
      Topic:
        Value: /nvblox_node/mesh
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
      Value: true

    - Class: rviz_default_plugins/PointCloud2
      Name: Point Cloud
      Enabled: true
      Alpha: 1
      Autocompute Intensity Bounds: true
      Autocompute Value Bounds:
        Max Value: 10
        Min Value: -10
        Value: true
      Axis: Z
      Channel Name: intensity
      Color: 255; 255; 255
      Color Transformer: RGB8
      Decay Time: 0
      Invert Rainbow: false
      Max Color: 255; 255; 255
      Min Color: 0; 0; 0
      Position Transformer: XYZ
      Selectable: true
      Size (Pixels): 2
      Size (m): 0.01
      Style: Points
      Topic:
        Value: /nvblox_node/esdf_pointcloud
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
      Use Timestamp: false
      Use rainbow: true
      Value: true

    - Class: rviz_default_plugins/TF
      Name: TF
      Enabled: true
      Frame Timeout: 15
      Marker Scale: 1
      Show Arrows: true
      Show Axes: true
      Show Names: true
      Update Interval: 0
      Frames:
        All Enabled: true
        map:
          Value: true
        odom:
          Value: true
        camera_0_optical_frame:
          Value: true
        camera_1_optical_frame:
          Value: true
      Tree:
        map:
          odom: {}
      Value: true

    - Class: rviz_default_plugins/Camera
      Name: RGB Camera 0
      Enabled: true
      Topic:
        Value: /camera_0/rgb/image_raw
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
      Camera Info Topic:
        Value: /camera_0/rgb/camera_info
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
      Overlay Alpha: 0.5
      Queue Size: 2
      Value: true

    - Class: rviz_default_plugins/Image
      Name: Depth Camera 0
      Enabled: false
      Max Value: 1
      Median window: 5
      Min Value: 0
      Normalize Range: true
      Topic:
        Value: /camera_0/depth/image_raw
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
      Unreliable: false
      Value: true

  Tools:
    - Class: rviz_default_plugins/Interact
      Hide Inactive Objects: true
    - Class: rviz_default_plugins/MoveCamera
    - Class: rviz_default_plugins/Select
    - Class: rviz_default_plugins/FocusCamera
    - Class: rviz_default_plugins/Measure
    - Class: rviz_default_plugins/SetInitialPose
      Topic:
        Value: /initialpose
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
    - Class: rviz_default_plugins/SetGoal
      Topic:
        Value: /goal_pose
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
    - Class: rviz_default_plugins/PublishPoint
      Single click: true
      Topic:
        Value: /clicked_point
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable

  Views:
    Current:
      Class: rviz_default_plugins/Orbit
      Name: Current View
      Distance: 10
      Focal Point:
        X: 0
        Y: 0
        Z: 0
      Focal Shape Fixed Size: true
      Focal Shape Size: 0.05000000074505806
      Invert Z Axis: false
      Near Clip Distance: 0.009999999776482582
      Pitch: 1.5697963237762451
      Target Frame: <Fixed Frame>
      Yaw: 0.7853981852531433
      Enable Stereo Rendering:
        Value: false
        Stereo Eye Separation: 0.05999999865889549
        Stereo Focal Distance: 1
        Swap Stereo Eyes: false
    Saved: ~

Window Geometry:
  Displays:
    collapsed: false
  Time:
    collapsed: false
  Hide Left Dock: false
  Hide Right Dock: false
  Height: 1080
  Width: 1920
  X: 72
  Y: 27
  QMainWindow State: 


